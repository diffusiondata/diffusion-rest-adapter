<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite use-query-string="true" default-match-type="regex" use-context="false">
    <rule>
        <from>^/createService$</from>
        <to type="forward" last="true">/index.html</to>
    </rule>
    <rule>
        <from>^/service/.*$</from>
        <to type="forward" last="true">/index.html</to>
    </rule>
</urlrewrite>
