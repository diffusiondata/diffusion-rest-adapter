{
  "name": "web-interface-client",
  "version": "0.0.1",
  "browserify-shim": {
    "diffusion": "global:diffusion",
    "diffusionConfig": "global:diffusionConfig"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "compile": "tsc",
    "test": "nyc ./node_modules/.bin/jasmine",
    "bundle": "browserify -e target/js/init.js -o target/client.js"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": [
      "src/main/ts/**/*.ts"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "report-dir": "./target/coverage",
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0
  },
  "dependencies": {
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/router": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "when": "3.7.8",
    "reflect-metadata": "0.1.12",
    "zone.js": "0.8.26",
    "rxjs": "5.5.8",
    "typescript": "3.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.7",
    "@types/mockery": "1.4.29",
    "@types/node": "10.14.22",
    "@types/saywhen": "1.1.0",
    "browserify": "13.1.0",
    "browserify-shim": "3.8.12",
    "diffusion": "6.1.6",
    "jasmine": "3.1.0",
    "mockery": "2.1.0",
    "nyc": "14.1.1",
    "saywhen": "1.1.0",
    "ts-node": "6.0.3"
  }
}
